{
    "name": "actb/repositories",
    "description": "Laravel Repositories",
    "license": "MIT",
    "homepage": "http://dukagj.in",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "repository",
        "repositories",
        "eloquent",
        "database"
    ],
    "authors": [
        {
            "name": "Dukagjin Surdulli",
            "email": "me@dukagj.in"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^5.5|^6",
        "illuminate/database": "^5.5|^6",
        "illuminate/filesystem": "^5.5|^6",
        "illuminate/console": "^5.5|^6"
    },
    "require-dev": {
        "mockery/mockery": "@stable",
        "phpunit/phpunit": "^8.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "ActivismeBe\\Repositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActivismeBe\\Repositories\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ActivismeBe\\Repositories\\Providers\\RepositoryProvider"
            ]
        }
    }
}
